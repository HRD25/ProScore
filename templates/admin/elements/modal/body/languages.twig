
{% for language in CONST_LANGUAGES|default([]) %}
    <div class="row mb-3">
        <label for="name-{{ language.short }}" class="col-sm-2 col-form-label">{{ language.title }}:</label>
        <div class="col-sm-10">
            <input type="text" class="form-control" id="name-{{ language.short }}">
        </div>
    </div>
{% endfor %}